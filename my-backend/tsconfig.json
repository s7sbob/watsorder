{
  "compilerOptions": {
    "strict": true,
    "module": "commonjs",
    "target": "es6",
    "esModuleInterop": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "typeRoots": ["node_modules/@types", "src/types"],
    "types": ["multer"]
  },
  "include": [
    "src",
    "src/**/*.ts",
    "src/types" // تأكد من إدراج ملف التعريف الجديد هنا
  ]
}
